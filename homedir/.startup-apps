#!/usr/bin/env bash

[ -f $HOME/.config/wall.jpg ] && feh --bg-scale $HOME/.config/wall.jpg &
setxkbmap -model abnt2 -layout br -option 'caps:ctrl_modifier' &
xcape -e 'Caps_Lock=Escape' &
#nm-applet &
clipmenud &
mpd &
transmission-daemon &
udiskie -2 -t &
volumeicon &
redshift-gtk &
dropbox start &
unclutter --timeout 2 &
[ -f $HOME/.Xdefaults ] && xrdb -load $HOME/.Xdefaults &
[ -f $HOME/.Xresources ] && xrdb -merge $HOME/.Xresources &
[ -f $HOME/workspace/outros/dotfiles/data/suckless-tools/xsetroot.sh ] && bash $HOME/workspace/outros/dotfiles/data/suckless-tools/xsetroot.sh &
python $HOME/.scripts/syncaf.py &
xset -b
xset s off -dpms
emacs &
qutebrowser &
